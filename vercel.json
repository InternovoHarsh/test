{
    "version": 2,
    "routes": [
      { "src": "/", "dest": "/index.js" },
      { "src": "/getNewsInvApi", "dest": "/index.js" },
      { "src": "/api/*", "dest": "/index.js" }
    ],
    "functions": {
      "api/getNewsInvApi": {
        "runtime": "nodejs14.x",
        "memory": 1024
      }
    },
    "builds": [
      {
        "src": "/index.js",
        "use": "@vercel/node"
      }
    ]
  }
  